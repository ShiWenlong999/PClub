<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>PClub</display-name>
  <welcome-file-list>
    <welcome-file>login.html</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>regist</servlet-name>
    <servlet-class>servlet.Regist</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>regist</servlet-name>
    <url-pattern>/regist</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>login</servlet-name>
    <servlet-class>servlet.Login</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>login</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>Addcourse</servlet-name>
    <servlet-class>servlet.AddCourse</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Addcourse</servlet-name>
    <url-pattern>/addCourse</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>SelCourse</servlet-name>
    <servlet-class>servlet.SelCourse</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>SelCourse</servlet-name>
    <url-pattern>/selCourse</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>SelAllCourse</servlet-name>
    <servlet-class>servlet.SelAllCourse</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>SelAllCourse</servlet-name>
    <url-pattern>/selAllCourse</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>ModifyCourse</servlet-name>
    <servlet-class>servlet.ModifyCourse</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ModifyCourse</servlet-name>
    <url-pattern>/modifyCourse</url-pattern>
  </servlet-mapping>
  
   <servlet>
    <servlet-name>UserServlet</servlet-name>
    <servlet-class>servlet.UserServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>UserServlet</servlet-name>
    <url-pattern>/userServlet</url-pattern>
  </servlet-mapping>
</web-app>